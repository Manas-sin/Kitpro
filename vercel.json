{
    "version": 2,
    "builds": [
      {
        "src": "*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/about",
        "dest": "/about.html"
      },
      {
        "src": "/server",
        "dest": "/server.html"
      }
    ]
  }